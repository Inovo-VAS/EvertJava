package inovo.queues;

public class Request implements Runnable {
	private Object _reqLock=new Object();
	@Override
	public void run() {
		if(this.executeRequest()){
			this._reqLock.wait(this.requestDelay());
		}
		else{
			_reqLock.notifyAll();
		}
	}
	
	public long requestDelay(){
		return 2;
	}

}
