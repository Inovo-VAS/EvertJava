				  }
			  }
			  
			  _paramFieldCount=maxColSize;
		  }
		  catch (Exception e){
			  e.printStackTrace();
		  }*/
	    
	    if(_messagingQueue==null) _messagingQueue=new MessagingQueue();
	    return _messagingQueue;
	  }

	  
