			 if(!this.canExecuteCoding()&&(this._codingIndexStage!=this.codingSpecificIndex("{")&&this._codingIndexStage!=this.codingSpecificIndex("}"))){
				 this._codingPrevIndexStage=this._codingIndexStage;
