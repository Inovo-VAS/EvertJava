				}
				else{
					this._maskCodeStage=maskCodeStage.noStage;
				}
			}
			else{
				this._maskCodeStage=maskCodeStage.noStage;
			}
			break;
		case codeBlockEnd:
			break;
		case commit:
			break;
		case elemName:
